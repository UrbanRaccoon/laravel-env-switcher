{
    "name": "vizrex/laravel-env-switcher",
    "description": "This package provides a command line utility to switch Laravel env files easily.",
    "type": "laravel-package",
    "repositories": [{
        "type": "path",
        "url": "../laraviz",
        "options": {
            "symlink": true
        }
    }],
    "authors": [{
        "name": "Vizrex",
        "email": "laravel@vizrex.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "illuminate/support": "~5",
        "vizrex/laraviz": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Vizrex\\LaravelEnvSwitcher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vizrex\\LaravelEnvSwitcher\\LaravelEnvSwitcherProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "dev"
        }
    }
}